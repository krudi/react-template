    "cssnano": "^6.0.1",
    "eslint": "^8.48.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^14.0.1",
    "markdownlint": "^0.28.2",
