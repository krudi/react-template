{
  "name": "react-template",
  "description": "A template with Next.js built on React with focus on performance and best practices.",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/krudi/react-template/LICENSE"
    }
  ],
  "version": "0.1.0",
  "authors": [
    {
      "name": "krudi",
      "email": "patryk.kudlik@gmail.com",
      "url": "https://github.com/krudi/react-template"
    }
  ],
  "private": false,
  "homepage": "https://github.com/krudi/react-template#readme",
  "bugs": {
    "url": "https://github.com/krudi/react-template/issues"
  },
  "keywords": [
    "template"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/krudi/react-template.git"
  },
  "engines": {
    "node": ">= 20.11.0",
    "npm": ">= 10.8.1"
  },
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo --show-all",
    "build": "next build",
    "start": "next start",
    "lint:js": "eslint --ext .js,.ts,.jsx,.tsx .",
    "lint:js:fix": "eslint --fix --ext .js,.ts,.jsx,.tsx .",
    "lint:css": "stylelint **/*.css",
    "lint:css:fix": "stylelint --fix **/*.css",
    "lint:files": "prettier --check \"**/*.{html,graphql,yml,yaml}\"",
    "lint:files:fix": "prettier --write \"**/*.{html,graphql,yml,yaml}\"",
    "lint:full": "npm run lint:js && npm run lint:css && npm run lint:files",
    "lint:full:fix": "npm run lint:js:fix && npm run lint:css:fix && npm run lint:files"
  },
  "dependencies": {
    "next": "14.2.3",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@babel/preset-react": "7.24.6",
    "@types/node": "^20.13.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.16.1",
    "@vitejs/plugin-react": "4.3.0",
    "autoprefixer": "10.4.19",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "postcss": "8.4.38",
    "postcss-custom-media": "10.0.6",
    "postcss-import": "16.1.0",
    "postcss-load-config": "5.1.0",
    "postcss-nested": "6.0.1",
    "postcss-syntax": "0.36.2",
    "prettier": "3.2.5",
    "prop-types": "15.8.1",
    "stylelint": "16.6.1",
    "stylelint-config-standard": "36.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
