    "prop-types": "^15.8.1",
    "sass": "^1.66.1",
    "start-server-and-test": "^2.0.0",
    "stylelint": "^15.10.3",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
