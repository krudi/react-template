    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "lint:jsx": "next lint",
    "lint:js": "eslint --ext .js,.ts,.jsx,.tsx .",
    "lint:js:fix": "eslint --fix --ext .js,.ts,.jsx,.tsx .",
    "lint:css": "stylelint **/*.{css,scss}",
    "lint:css:fix": "stylelint --fix **/*.{css,scss}",
    "lint:files": "prettier --check \"**/*.{html,graphql,yml,yaml,vue}\"",
    "lint:files:fix": "prettier --write \"**/*.{html,graphql,yml,yaml,vue}\"",
    "lint:markdown": "markdownlint **/*.md ",
    "lint:full": "npm run lint:js && npm run lint:css && npm run lint:files && npm run lint:markdown && npm run lint:jsx",
    "lint:full:fix": "npm run lint:js:fix && npm run lint:css:fix && npm run lint:files && npm run lint:markdown",
    "jest:run": "jest",
    "deploy": "npm run build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@types/node": "^20.5.9",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
