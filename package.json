{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "react-template",
    "version": "0.1.0",
    "private": false,
    "description": "A template with Next.js built on React with focus on performance and best practices.",
    "keywords": [
        "template"
    ],
    "homepage": "https://github.com/krudi/react-template#readme",
    "bugs": {
        "url": "https://github.com/krudi/react-template/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/krudi/react-template.git"
    },
    "license": "MIT",
    "author": {
        "name": "krudi",
        "email": "patryk.kudlik@gmail.com",
        "url": "https://github.com/krudi"
    },
    "type": "module",
    "scripts": {
        "clean": "rm -rf .cache node_modules .next",
        "build": "next build",
        "dev": "next dev",
        "dev:turbo": "next dev --turbo",
        "lint:eslint": "eslint . --cache --cache-location .cache/.eslintcache",
        "lint:eslint:fix": "eslint . --fix --cache --cache-location .cache/.eslintcache",
        "lint:prettier": "prettier --check . --cache --cache-strategy metadata --cache-location .cache/.prettiercache",
        "lint:prettier:fix": "prettier --write . --cache --cache-strategy metadata --cache-location .cache/.prettiercache",
        "lint:stylelint": "stylelint --cache --cache-location .cache/.stylelintcache -c ./stylelint.config.mjs **/*.css",
        "lint:stylelint:fix": "stylelint --cache --cache-location .cache/.stylelintcache -c ./stylelint.config.mjs --fix **/*.css",
        "start": "next start",
        "typecheck": "tsc -p tsconfig.json --noEmit"
    },
    "dependencies": {
        "next": "^16.1.6",
        "react": "^19.2.4",
        "react-dom": "^19.2.4"
    },
    "devDependencies": {
        "@babel/preset-react": "^7.28.5",
        "@krudi/eslint-config": "^0.1.8",
        "@krudi/prettier-config": "^0.1.8",
        "@krudi/stylelint-config": "^0.1.1",
        "@krudi/typescript-config": "^0.1.9",
        "@types/node": "^24.10.15",
        "@types/react": "^19.2.14",
        "@types/react-dom": "^19.2.3",
        "autoprefixer": "^10.4.27",
        "postcss": "^8.5.6",
        "postcss-import": "^16.1.1",
        "postcss-load-config": "^6.0.1",
        "postcss-nesting": "^13.0.2",
        "postcss-syntax": "^0.36.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3"
    },
    "packageManager": "npm@11.6.4",
    "engines": {
        "node": ">= 22.18.0",
        "npm": ">= 11.6.4"
    }
}
