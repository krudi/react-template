{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "react-template",
    "description": "A template with Next.js built on React with focus on performance and best practices.",
    "version": "0.1.0",
    "license": "MIT",
    "author": {
        "name": "krudi",
        "email": "patryk.kudlik@gmail.com",
        "url": "https://github.com/krudi"
    },
    "private": false,
    "homepage": "https://github.com/krudi/react-template#readme",
    "bugs": {
        "url": "https://github.com/krudi/react-template/issues"
    },
    "keywords": [
        "template"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/krudi/react-template.git"
    },
    "engines": {
        "node": ">= 22.18.0",
        "npm": ">= 11.6.2"
    },
    "packageManager": "npm@11.6.2",
    "type": "module",
    "scripts": {
        "dev": "next dev",
        "dev:turbo": "next dev --turbo",
        "build": "next build",
        "start": "next start",
        "typecheck": "tsc -p tsconfig.json --noEmit",
        "lint:eslint": "eslint . --cache --cache-location .cache/.eslintcache",
        "lint:eslint:fix": "eslint . --fix --cache --cache-location .cache/.eslintcache",
        "lint:stylelint": "stylelint --c ./stylelint.config.mjs **/*.css",
        "lint:stylelint:fix": "stylelint -c ./stylelint.config.mjs --fix **/*.css"
    },
    "dependencies": {
        "next": "^16.0.6",
        "react": "^19.2.0",
        "react-dom": "^19.2.0"
    },
    "devDependencies": {
        "@babel/preset-react": "^7.28.5",
        "@krudi/eslint-config": "^0.1.7",
        "@krudi/prettier-config": "^0.1.6",
        "@krudi/typescript-config": "^0.1.9",
        "@types/node": "^24.10.1",
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "autoprefixer": "^10.4.22",
        "postcss": "^8.5.6",
        "postcss-import": "^16.1.1",
        "postcss-load-config": "^6.0.1",
        "postcss-nested": "^7.0.2",
        "postcss-syntax": "^0.36.2",
        "stylelint": "^16.26.1",
        "stylelint-config-standard": "^39.0.1",
        "stylelint-order": "^7.0.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3"
    }
}
