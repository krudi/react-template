{
  "name": "react-template",
  "description": "A template with Next.js built on React with focus on performance and best practices.",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/krudi/react-template/LICENSE"
    }
  ],
  "version": "0.1.0",
  "publisher": "author",
  "authors": [
    {
      "name": "authors",
      "email": "example@mail.com",
      "url": "https://github.com/krudi/react-template"
    }
  ],
  "private": false,
  "homepage": "https://github.com/krudi/react-template#readme",
  "bugs": {
    "url": "https://github.com/krudi/react-template/issues"
  },
  "keywords": [
    "template"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/krudi/react-template.git"
  },
  "funding": [
    {
      "type": "individual",
      "url": "http://example.com/donate"
    }
  ],
  "engines": {
    "node": ">= 18.17.1"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "lint:jsx": "next lint",
    "lint:js": "eslint --ext .js,.ts,.jsx,.tsx .",
    "lint:js:fix": "eslint --fix --ext .js,.ts,.jsx,.tsx .",
    "lint:css": "stylelint **/*.{css,scss}",
    "lint:css:fix": "stylelint --fix **/*.{css,scss}",
    "lint:files": "prettier --check \"**/*.{html,graphql,yml,yaml,vue}\"",
    "lint:files:fix": "prettier --write \"**/*.{html,graphql,yml,yaml,vue}\"",
    "lint:markdown": "markdownlint **/*.md ",
    "lint:full": "npm run lint:js && npm run lint:css && npm run lint:files && npm run lint:markdown && npm run lint:jsx",
    "lint:full:fix": "npm run lint:js:fix && npm run lint:css:fix && npm run lint:files && npm run lint:markdown",
    "cy:verify": "cypress verify",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:e2e": "start-server-and-test dev http://localhost:3000 \"cypress open --e2e\"",
    "cy:e2e:headless": "start-server-and-test dev http://localhost:3000 \"cypress run --e2e\"",
    "cy:component": "cypress open --component",
    "cy:component:headless": "cypress run --component",
    "jest:run": "jest",
    "deploy": "npm run build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "next": "13.4.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.22.3",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^29.5.0",
    "cssnano": "^6.0.1",
    "cypress": "^12.13.0",
    "eslint": "^8.41.0",
    "eslint-config-next": "13.4.13",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^13.2.3",
    "markdownlint": "^0.28.2",
    "markdownlint-cli": "^0.34.0",
    "postcss": "^8.4.24",
    "postcss-import": "^15.1.0",
    "postcss-load-config": "^4.0.1",
    "postcss-nested": "^6.0.1",
    "postcss-scss": "^4.0.6",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.8.8",
    "prop-types": "^15.8.1",
    "sass": "^1.62.1",
    "start-server-and-test": "^2.0.0",
    "stylelint": "^15.6.2",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1"
  }
}
