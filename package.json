{
  "name": "react-template",
  "description": "A template with Next.js built on React with focus on performance and best practices.",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/krudi/react-template/LICENSE"
    }
  ],
  "version": "0.1.0",
  "publisher": "author",
  "authors": [
    {
      "name": "authors",
      "email": "example@mail.com",
      "url": "https://github.com/krudi/react-template"
    }
  ],
  "private": false,
  "homepage": "https://github.com/krudi/react-template#readme",
  "bugs": {
    "url": "https://github.com/krudi/react-template/issues"
  },
  "keywords": [
    "template"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/krudi/react-template.git"
  },
  "funding": [
    {
      "type": "individual",
      "url": "http://example.com/donate"
    }
  ],
  "engines": {
    "node": ">= 20.11.0"
  },
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo --show-all",
    "build": "next build",
    "start": "next start",
    "lint:js": "eslint --ext .js,.ts,.jsx,.tsx .",
    "lint:js:fix": "eslint --fix --ext .js,.ts,.jsx,.tsx .",
    "lint:css": "stylelint **/*.{css,scss}",
    "lint:css:fix": "stylelint --fix **/*.{css,scss}",
    "lint:files": "prettier --check \"**/*.{html,graphql,yml,yaml}\"",
    "lint:files:fix": "prettier --write \"**/*.{html,graphql,yml,yaml}\"",
    "lint:full": "npm run lint:js && npm run lint:css && npm run lint:files",
    "lint:full:fix": "npm run lint:js:fix && npm run lint:css:fix && npm run lint:files",
    "deploy": "npm run build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@types/node": "^20.8.9",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "next": "14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.22.15",
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.9.0",
    "@vitejs/plugin-react": "^4.1.0",
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.1",
    "eslint": "^8.52.0",
    "eslint-config-next": "14.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "postcss": "^8.4.31",
    "postcss-import": "^15.1.0",
    "postcss-load-config": "^4.0.1",
    "postcss-nested": "^6.0.1",
    "postcss-scss": "^4.0.9",
    "postcss-syntax": "^0.36.2",
    "prettier": "^3.0.3",
    "prop-types": "^15.8.1",
    "sass": "^1.69.5",
    "start-server-and-test": "^2.0.3",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "ts-node": "^10.9.1"
  }
}
